<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "http://docs.oasis-open.org/dita/v1.1/OS/dtd/task.dtd">
<task id="taskId">
    <title>Refresh Project</title>
    <shortdesc>Refresh <wintitle>Project Manager</wintitle> window to reflect changes.</shortdesc>
    <taskbody>
        <context>From the <wintitle>Project Manager</wintitle> window:</context>
        <steps>
            <step>
                <cmd>Choose the <menucascade>
                    <uicontrol>Project</uicontrol><uicontrol>Refresh</uicontrol>
                </menucascade> menu item or press <userinput>F5</userinput>.</cmd>
            </step>
        </steps>
    </taskbody>
</task>