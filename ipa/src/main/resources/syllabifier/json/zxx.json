{
  "name": "Babbling",
  "language": "zxx",
  "sonority": [
    {
      "pattern": "[{obstruent}]",
      "sonority": 0
    },
    {
      "pattern": "[{c, nasal}]",
      "sonority": 1
    },
    {
      "pattern": "[{liquid}]",
      "sonority": 2
    },
    {
      "pattern": "[{g}]",
      "sonority": 3
    },
    {
      "pattern": "[{v}]",
      "sonority": 4
    }
  ],
  "steps": [
    {
      "name": "MarkNuclei #1",
      "pattern": "(D={v, -long}:U{v, -low}:U)"
    },
    {
      "name": "MarkNuclei #2",
      "pattern": "(N={v}:U)"
    },
    {
      "name": "MarkNuclei #3",
      "pattern": "(N=\u0027.*[\\u0329].*\u0027)"
    },
    {
      "name": "MarkOnset #1",
      "pattern": "(O=\\c:U).:N"
    },
    {
      "name": "MarkOnset #2",
      "pattern": ".:sctype(\"-O\")?(O={g}:N).:N"
    },
    {
      "name": "MarkOnset #3",
      "pattern": "^(O=j:N)"
    },
    {
      "name": "MaxOnset #1",
      "pattern": "(O={c,-strident}:U).:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MaxOnset #2",
      "pattern": "(O={strident, -anterior}:U).:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MarkCoda #1",
      "pattern": ".:N(C=\\c:U)"
    },
    {
      "name": "MaxCoda #1",
      "pattern": ".:C(C={c}:U:mdc(\"-1\",\"false\"))"
    },
    {
      "name": "Fix Branching Codas #1",
      "pattern": ".:N(C={obs,-cor}:U)"
    },
    {
      "name": "MarkLA #1",
      "pattern": "\\b\\s?(LA={strident}).:O"
    },
    {
      "name": "MarkLA #2",
      "pattern": ".:C(LA={strident}).:O"
    },
    {
      "name": "MarkRA #1",
      "pattern": ".:C(R={cor}:U)"
    },
    {
      "name": "MaxRA #1",
      "pattern": ".:R(R={cor}:U+)"
    },
    {
      "name": "Make Simple #1",
      "pattern": "(C=.:E+)"
    },
    {
      "name": "Make Simple #2",
      "pattern": "(O=.:L+)"
    },
    {
      "name": "Make Simple #3",
      "pattern": "(C=.:R+)"
    }
  ]
}