{
  "name": "Dutch-simple",
  "language": "nld-simple",
  "sonority": [
    {
      "pattern": "[{affricate}{stop,-nasal}{fricative, -uvular}{uvular,-fri}]",
      "sonority": 0
    },
    {
      "pattern": "[{nasal}]",
      "sonority": 1
    },
    {
      "pattern": "[{liquid}{uvular,fri}]",
      "sonority": 2
    },
    {
      "pattern": "[{g}]",
      "sonority": 3
    },
    {
      "pattern": "[{v}]",
      "sonority": 4
    }
  ],
  "steps": [
    {
      "name": "MarkNuclei #1",
      "pattern": "(D={v,-long}[É™{g,-stop,-fri,-affricate,-liq}{v,high}])"
    },
    {
      "name": "MarkNuclei #2",
      "pattern": "(D=j{v,-long}{v,high,lax})"
    },
    {
      "name": "MarkNuclei #3",
      "pattern": "(N=\\v)"
    },
    {
      "name": "MarkNuclei #4",
      "pattern": "(N=\u0027.*[\\u0329].*\u0027)"
    },
    {
      "name": "MarkOnset #1",
      "pattern": "(O=\\c:U).:N"
    },
    {
      "name": "MarkOnset #2",
      "pattern": ".:sctype(\"-O\")?(O={g}:N).:N"
    },
    {
      "name": "MaxOnset #1",
      "pattern": "(O={c,-strident}:U)\\c:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MaxOnset #2",
      "pattern": "(O={strident, -anterior}:U)\\c:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MaxOnsetCoronalNasal #1",
      "pattern": "(O=\\c:U){cor}:O:mdc(\"1\",\"false\")"
    },
    {
      "name": "MarkCoda #1",
      "pattern": ".:N(C=\\c:U)"
    },
    {
      "name": "MaxCoda #1",
      "pattern": ".:C(C={c}:U:mdc(\"-1\",\"false\"))"
    },
    {
      "name": "Fix Branching Codas #1",
      "pattern": ".:N(C={obs,-cor}:U)"
    },
    {
      "name": "MarkLA #1",
      "pattern": "(L={strident,voiceless}:U).:O"
    },
    {
      "name": "Coda-s-ComplexOnset #1",
      "pattern": ".:C(C=s)s"
    },
    {
      "name": "Coda-s-ComplexOnset #2",
      "pattern": ".:C(C=s).:O\u003c2\u003e"
    },
    {
      "name": "MarkRA #1",
      "pattern": "\\c:C(R={cor}:U+)"
    },
    {
      "name": "MarkLeftoverOnsets #1",
      "pattern": "(O=\\c:U)"
    },
    {
      "name": "FixLARA #1",
      "pattern": "(O=.:L+)"
    },
    {
      "name": "FixLARA #2",
      "pattern": "(C=.:R+)"
    }
  ]
}