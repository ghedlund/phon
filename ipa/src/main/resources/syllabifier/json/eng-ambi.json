{
  "name": "English-ambi",
  "language": "eng-ambi",
  "sonority": [
    {
      "pattern": "[{obstruent}]",
      "sonority": 0
    },
    {
      "pattern": "[{c, nasal}]",
      "sonority": 1
    },
    {
      "pattern": "[{liquid}]",
      "sonority": 2
    },
    {
      "pattern": "[{g}]",
      "sonority": 3
    },
    {
      "pattern": "[{v}]",
      "sonority": 4
    }
  ],
  "steps": [
    {
      "name": "MarkNuclei #1",
      "pattern": "\\c(D=ju)"
    },
    {
      "name": "MarkNuclei #2",
      "pattern": "(N={v}:U)"
    },
    {
      "name": "MarkNuclei #3",
      "pattern": "(D=\\v{v,high,lax})"
    },
    {
      "name": "MarkNuclei #4",
      "pattern": "(N=\u0027.*[\\u0329].*\u0027)"
    },
    {
      "name": "MarkOnset #1",
      "pattern": "(O=\\c:U).:N"
    },
    {
      "name": "MaxOnset #1",
      "pattern": "(O={c,-strident}:U)\\c:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MaxOnset #2",
      "pattern": "(O={c,strident,-anterior}:U)\\c:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MarkCoda #1",
      "pattern": ".:N(C=\\c:U)"
    },
    {
      "name": "MaxCoda #1",
      "pattern": ".:C(C={c}:U:mdc(\"-1\",\"false\"))"
    },
    {
      "name": "MarkLA #1",
      "pattern": "\\S(LA={anterior,strident,voiceless}:U).:O"
    },
    {
      "name": "MarkRA #1",
      "pattern": ".:C(R={cor}:U)"
    },
    {
      "name": "MaxRA #1",
      "pattern": ".:R(R={cor}:U+)"
    },
    {
      "name": "MarkOEHS #1",
      "pattern": "(E=\\c:U)"
    },
    {
      "name": "Ambisyllabicity #1",
      "pattern": "\\c+*.:N+(A=\\c:O).:N+\n"
    }
  ]
}