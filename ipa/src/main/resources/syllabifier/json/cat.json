{
  "name": "Catalan",
  "language": "cat",
  "sonority": [
    {
      "pattern": "{obs}",
      "sonority": 0
    },
    {
      "pattern": "{c,nasal}",
      "sonority": 1
    },
    {
      "pattern": "{liquid}",
      "sonority": 2
    },
    {
      "pattern": "\\g",
      "sonority": 3
    },
    {
      "pattern": "\\v",
      "sonority": 4
    }
  ],
  "steps": [
    {
      "name": "MarkNuclei #1",
      "pattern": "(D=\\g\\v)"
    },
    {
      "name": "MarkNuclei #2",
      "pattern": "(D=\\v[{v,high,lax}{g,-stop,-fricative,-affricate,-liquid}])"
    },
    {
      "name": "MarkNuclei #3",
      "pattern": "(D={v, high, lax}\\v)"
    },
    {
      "name": "MarkNuclei #4",
      "pattern": "(N=\\v:U)"
    },
    {
      "name": "MarkOnset #1",
      "pattern": "(O=\\c:U).:N"
    },
    {
      "name": "MarkOnset #2",
      "pattern": ".:sctype(\"-O\")?(O={g}:N).:N"
    },
    {
      "name": "MaxOnset #1",
      "pattern": "(O=\\c:U).:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MarkCoda #1",
      "pattern": ".:N(C=\\c:U)"
    },
    {
      "name": "MaxCoda #1",
      "pattern": ".:C(C={c}:U:mdc(\"-1\",\"false\"))"
    },
    {
      "name": "MarkRA #1",
      "pattern": ".:C(R={cor}:U)"
    },
    {
      "name": "MaxRA #1",
      "pattern": ".:R(R={cor}:U+)"
    }
  ]
}