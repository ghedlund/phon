{
  "name": "Icelandic",
  "language": "isl",
  "sonority": [
    {
      "pattern": "[{obstruent}]",
      "sonority": 0
    },
    {
      "pattern": "[{c, nasal}]",
      "sonority": 1
    },
    {
      "pattern": "[{liquid}]",
      "sonority": 2
    },
    {
      "pattern": "[{g}]",
      "sonority": 3
    },
    {
      "pattern": "[{v}]",
      "sonority": 4
    }
  ],
  "steps": [
    {
      "name": "MarkNuclei #1",
      "pattern": "(D=\\v[É™{v,high}])"
    },
    {
      "name": "MarkNuclei #2",
      "pattern": "(N=\\v)"
    },
    {
      "name": "MarkNuclei #3",
      "pattern": "(N=\u0027.*[\\u0329].*\u0027)"
    },
    {
      "name": "MarkOnset #1",
      "pattern": "(O=\\c:U).:N"
    },
    {
      "name": "MaxOnset #1",
      "pattern": "(O={c,-strident}:U){c, -nasal}:O:mdc(\"2\",\"false\")"
    },
    {
      "name": "MaxOnset #2",
      "pattern": "\\b(O={c,-strident}:U).:O:mdc(\"1\",\"true\")"
    },
    {
      "name": "MarkCoda #1",
      "pattern": ".:N(C=\\c:U)"
    },
    {
      "name": "MarkCoda #2",
      "pattern": "h:C(C=\\c:U){nas}:O"
    },
    {
      "name": "MarkLA #1",
      "pattern": "^(L={anterior,strident,voiceless}:U)"
    },
    {
      "name": "MarkLA #2",
      "pattern": ".:sctype(\"Coda|SyllableStressMarker|SyllableBoundaryMarker|Nucleus\")(L={anterior,strident,voiceless}:U).:O"
    },
    {
      "name": "MarkRA #1",
      "pattern": ".:C(R=.:U+)"
    },
    {
      "name": "Excrescent Cs #1",
      "pattern": "\\c(O=\\c:U)\\c"
    }
  ]
}