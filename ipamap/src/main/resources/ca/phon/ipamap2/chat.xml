<ipaGrids xmlns="http://phon.ling.mun.ca/ns/ipamap">

    <grid rows="2" cols="44" name="Utterance linkers">
        <cell x="0" y="0" w="4" h="2">
            <text>+"</text>
            <property name="insert">+"</property>
            <property name="Name">Quoted utterance next</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#QuotedUtterance_Linker</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>+^</text>
            <property name="insert">+^</property>
            <property name="Name">Quick uptake</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#QuickUptake_Linker</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>+&lt;</text>
            <property name="insert">+&lt;</property>
            <property name="Name">Lazy overlap mark</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#LazyOverlap_Linker</property>
        </cell>
        <cell x="12" y="0" w="4" h="2">
            <text>+,</text>
            <property name="insert">+,</property>
            <property name="Name">Self completion</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#SelfCompletion_Linker</property>
        </cell>
        <cell x="16" y="0" w="4" h="2">
            <text>++</text>
            <property name="insert">++</property>
            <property name="Name">Other completion</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#OtherCompletion_Linker</property>
        </cell>
        <cell x="20" y="0" w="4" h="2">
            <text>+&#x224B;</text>
            <property name="insert">+&#x224B;</property>
            <property name="Name">Technical break TCU continuation</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#TechnicalBreakTCUCompletion_Linker</property>
        </cell>
        <cell x="24" y="0" w="4" h="2">
            <text>+&#x2248;</text>
            <property name="insert">+&#x2248;</property>
            <property name="Name">No break TCU continuation</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#NoBreakTCUCompletion_Linker</property>
        </cell>
    </grid>

    <grid cols="44" rows="4" name="Utterance terminators">
        <cell x="0" y="0" w="4" h="2">
            <text>.</text>
            <property name="insert">.</property>
            <property name="Name">sentence terminator</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>!</text>
            <property name="insert">!</property>
            <property name="Name">exclamation</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>?</text>
            <property name="insert">?</property>
            <property name="Name">question</property>
        </cell>
        <cell x="12" y="0" w="4" h="2">
            <text>+.</text>
            <property name="insert">+.</property>
            <property name="Name">broken for coding</property>
        </cell>
        <cell x="16" y="0" w="4" h="2">
            <text>+...</text>
            <property name="insert">+...</property>
            <property name="Name">trail off</property>
        </cell>
        <cell x="20" y="0" w="4" h="2">
            <text>+..?</text>
            <property name="insert">+..?</property>
            <property name="Name">trail off question</property>
        </cell>
        <cell x="24" y="0" w="4" h="2">
            <text>+!?</text>
            <property name="insert">+!?</property>
            <property name="Name">question exclamation</property>
        </cell>
        <cell x="28" y="0" w="4" h="2">
            <text>+!.</text>
            <property name="insert">+!.</property>
            <property name="Name">exclamation sentence</property>
        </cell>
        <cell x="32" y="0" w="4" h="2">
            <text>+/.</text>
            <property name="insert">+/.</property>
            <property name="Name">interruption</property>
        </cell>
        <cell x="36" y="0" w="4" h="2">
            <text>+/?</text>
            <property name="insert">+/?</property>
            <property name="Name">interruption question</property>
        </cell>
        <cell x="40" y="0" w="4" h="2">
            <text>+//.</text>
            <property name="insert">+//.</property>
            <property name="Name">self interruption</property>
        </cell>

        <cell x="0" y="2" w="4" h="2">
            <text>+//?</text>
            <property name="insert">+//?</property>
            <property name="Name">self interruption question</property>
        </cell>
        <cell x="4" y="2" w="4" h="2">
            <text>+"/.</text>
            <property name="insert">+"/.</property>
            <property name="Name">quotation next line</property>
        </cell>
        <cell x="8" y="2" w="4" h="2">
            <text>+".</text>
            <property name="insert">+"/?</property>
            <property name="Name">quotation precedes</property>
        </cell>
        <cell x="12" y="2" w="4" h="2">
            <text>&#x224B;</text>
            <property name="insert">&#x224B;</property>
            <property name="Name">technical break TCU continuation</property>
        </cell>
        <cell x="16" y="2" w="4" h="2">
            <text>&#x2248;</text>
            <property name="insert">&#x2248;</property>
            <property name="Name">no break TCU continuation</property>
        </cell>
        <cell x="20" y="2" w="4" h="2">
            <text>[+ ]</text>
            <property name="insert">[+ $$text$$]</property>
            <property name="Name">Postcode</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Postcodes</property>
        </cell>
    </grid>

     <grid rows="2" cols="44" name="Word prefix">
         <cell x="0" y="0" w="4" h="2">
             <text>0</text>
             <property name="insert">0$$word$$</property>
             <property name="Name">omission</property>
         </cell>
         <cell x="4" y="0" w="4" h="2">
             <text>&amp;~</text>
             <property name="insert">&amp;~$$word$$</property>
             <property name="Name">non word</property>
        </cell>
         <cell x="8" y="0" w="4" h="2">
             <text>&amp;-</text>
             <property name="insert">&amp;-$$word$$</property>
             <property name="Name">filler</property>
         </cell>
         <cell x="12" y="0" w="4" h="2">
            <text>&amp;+</text>
            <property name="insert">&amp;+$$word$$</property>
            <property name="Name">fragment</property>
         </cell>
     </grid>

    <grid rows="6" cols="44" name="Word suffix">
        <cell x="0" y="0" w="4" h="2">
            <text>#</text>
            <property name="insert">$$word$$#</property>
            <property name="Name">separated prefix</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>@a</text>
            <property name="insert">$$word$$@a</property>
            <property name="Name">addition</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>@b</text>
            <property name="insert">$$word$$@b</property>
            <property name="Name">babbling</property>
        </cell>
        <cell x="12" y="0" w="4" h="2">
            <text>@c</text>
            <property name="insert">$$word$$@c</property>
            <property name="Name">child-invented form</property>
        </cell>
        <cell x="16" y="0" w="4" h="2">
            <text>@d</text>
            <property name="insert">$$word$$@d</property>
            <property name="Name">dialect form</property>
        </cell>
        <cell x="20" y="0" w="4" h="2">
            <text>@e</text>
            <property name="insert">$$word$$@e</property>
            <property name="Name">echolalia</property>
        </cell>
        <cell x="24" y="0" w="4" h="2">
            <text>@f</text>
            <property name="insert">$$word$$@f</property>
            <property name="Name">family specific form</property>
        </cell>
        <cell x="28" y="0" w="4" h="2">
            <text>@fp</text>
            <property name="insert">$$word$$@fp</property>
            <property name="Name">filled pause</property>
        </cell>
        <cell x="32" y="0" w="4" h="2">
            <text>@g</text>
            <property name="insert">$$word$$@g</property>
            <property name="Name">general special form</property>
        </cell>
        <cell x="36" y="0" w="4" h="2">
            <text>@i</text>
            <property name="insert">$$word$$@i</property>
            <property name="Name">interjection, interaction</property>
        </cell>
        <cell x="40" y="0" w="4" h="2">
            <text>@k</text>
            <property name="insert">$$word$$@k</property>
            <property name="Name">multiple letters</property>
        </cell>
        <cell x="0" y="2" w="4" h="2">
            <text>@l</text>
            <property name="insert">$$word$$@l</property>
            <property name="Name">letter</property>
        </cell>
        <cell x="4" y="2" w="4" h="2">
            <text>@n</text>
            <property name="insert">$$word$$@n</property>
            <property name="Name">neologism</property>
        </cell>
        <cell x="8" y="2" w="4" h="2">
            <text>@nv</text>
            <property name="insert">$$word$$@nv</property>
            <property name="Name">no voice</property>
        </cell>
        <cell x="12" y="2" w="4" h="2">
            <text>@o</text>
            <property name="insert">$$word$$@o</property>
            <property name="Name">onomatopoeia</property>
        </cell>
        <cell x="16" y="2" w="4" h="2">
            <text>@p</text>
            <property name="insert">$$word$$@p</property>
            <property name="Name">phonology consistent form</property>
        </cell>
        <cell x="20" y="2" w="4" h="2">
            <text>@q</text>
            <property name="insert">$$word$$@q</property>
            <property name="Name">quoted metareference</property>
        </cell>
        <cell x="24" y="2" w="4" h="2">
            <text>@s:*</text>
            <property name="insert">$$word$$@s:*</property>
            <property name="Name">second-language form</property>
        </cell>
        <cell x="28" y="2" w="4" h="2">
            <text>@s$n</text>
            <property name="insert">$$word$$@s$n</property>
            <property name="Name">second language noun</property>
        </cell>
        <cell x="32" y="2" w="4" h="2">
            <text>@si</text>
            <property name="insert">$$word$$@si</property>
            <property name="Name">singing</property>
        </cell>
        <cell x="36" y="2" w="4" h="2">
            <text>@sl</text>
            <property name="insert">$$word$$@sl</property>
            <property name="Name">signed language</property>
        </cell>
        <cell x="40" y="2" w="4" h="2">
            <text>@sas</text>
            <property name="insert">$$word$$@sas</property>
            <property name="Name">sign and speech</property>
        </cell>
        <cell x="0" y="4" w="4" h="2">
            <text>@t</text>
            <property name="insert">$$word$$@t</property>
            <property name="Name">test form</property>
        </cell>
        <cell x="4" y="4" w="4" h="2">
            <text>@u</text>
            <property name="insert">$$word$$@u</property>
            <property name="Name">Unibet transcription</property>
        </cell>
        <cell x="8" y="4" w="4" h="2">
            <text>@wp</text>
            <property name="insert">$$word$$@wp</property>
            <property name="Name">word play</property>
        </cell>
        <cell x="12" y="4" w="4" h="2">
            <text>@x</text>
            <property name="insert">$$word$$@x</property>
            <property name="Name">excluded words</property>
        </cell>

        <cell x="16" y="4" w="4" h="2">
            <text>@z:*</text>
            <property name="insert">$$word$$@z:xxx</property>
            <property name="Name">user-defined code</property>
        </cell>
    </grid>



    <grid cols="44" rows="2" name="Grouping">
        <cell x="0" y="0" w="4" h="2">
            <text>&lt;&gt;</text>
            <property name="insert">&lt;$$&gt;</property>
            <property name="Name">Word group</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>‹›</text>
            <property name="insert">‹$$›</property>
            <property name="Name">Phonological group</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>()</text>
            <property name="insert">($$)</property>
            <property name="Name">Shortening</property>
        </cell>
    </grid>

    <grid cols="44" rows="2" name="Markers">
        <cell x="0" y="0" w="4" h="2">
            <text>[!]</text>
            <property name="insert">[!]</property>
            <property name="Name">Stressing</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Stressing_Scope</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>[!!]</text>
            <property name="insert">[!!]</property>
            <property name="Name">Contrastive stressing</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#ContrastiveStressing_Scope</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>[?]</text>
            <property name="insert">[?]</property>
            <property name="Name">Best guesss</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#BestGuess_Scope</property>
        </cell>
        <cell x="12" y="0" w="4" h="2">
            <text>[/]</text>
            <property name="insert">[/]</property>
            <property name="Name">Repetition</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Repetition_Scope</property>
        </cell>
        <cell x="16" y="0" w="4" h="2">
            <text>[//]</text>
            <property name="insert">[//]</property>
            <property name="Name">Retracing with correction</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Retracing_Scope</property>
        </cell>
        <cell x="20" y="0" w="4" h="2">
            <text>[///]</text>
            <property name="insert">[///]</property>
            <property name="Name">Retracing reformulation</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Reformulation_Scope</property>
        </cell>
        <cell x="24" y="0" w="4" h="2">
            <text>[/?]</text>
            <property name="insert">[/?]</property>
            <property name="Name">Retracing unclear</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#UnclearRetracing_Scope</property>
        </cell>
        <cell x="28" y="0" w="4" h="2">
            <text>[/-]</text>
            <property name="insert">[/-]</property>
            <property name="Name">False start</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#FalseStart_Scope</property>
        </cell>
        <cell x="32" y="0" w="4" h="2">
            <text>[e]</text>
            <property name="insert">[e]</property>
            <property name="Name">mor exclude</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#MorExclude_Scope</property>
        </cell>
    </grid>

    <grid cols="44" rows="2" name="Group annotations">
        <cell x="0" y="0" w="4" h="2">
            <text>[=? ]</text>
            <property name="insert">[=? $$word$$]</property>
            <property name="Name">Alternative transcription</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#AlternativeTranscription_Scope</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>[% ]</text>
            <property name="insert">[% $$text$$]</property>
            <property name="Name">Comment</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Comment_Scope</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>[= ]</text>
            <property name="insert">[= $$text$$]</property>
            <property name="Name">Explanation</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Explanation_Scope</property>
        </cell>
        <cell x="12" y="0" w="4" h="2">
            <text>[=! ]</text>
            <property name="insert">[=! $$text$$]</property>
            <property name="Name">Paralinguistics</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#ParalinguisticMaterial_Scope</property>
        </cell>
        <cell x="16" y="0" w="4" h="2">
            <text>[# ]</text>
            <property name="insert">[# $$...$$]</property>
            <property name="Name">Duration</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#Duration_Scope</property>
        </cell>
        <cell x="20" y="0" w="4" h="2">
            <text>[* ]</text>
            <property name="insert">[* $$text$$]</property>
            <property name="Name">Error</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#ErrorMarking_Scope</property>
        </cell>
        <cell x="24" y="0" w="4" h="2">
            <text>[^ ]</text>
            <property name="insert">[^ $$text$$]</property>
            <property name="Name">Freecode</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#ComplexLocalEvents</property>
        </cell>
    </grid>

    <grid rows="2" cols="44" name="Overlaps">
        <cell x="0" y="0" w="4" h="2">
            <text>[&lt;]</text>
            <property name="insert">[&lt;]</property>
            <property name="Name">Overlap precedes</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#OverlapPrecedes_Scope</property>
        </cell>
        <cell x="4" y="0" w="4" h="2">
            <text>[&gt;]</text>
            <property name="insert">[&gt;]</property>
            <property name="Name">Overlap follows</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#OverlapFollows_Scope</property>
        </cell>
        <cell x="8" y="0" w="4" h="2">
            <text>&#x2308;</text>
            <property name="insert">&#x2308;</property>
            <property name="Name">Top begin overlap</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#TopBeginOverlap_Delimiter</property>
        </cell>
        <cell x="12" y="0" w="4" h="2">
            <text>&#x2309;</text>
            <property name="insert">&#x2309;</property>
            <property name="Name">Top end overlap</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#TopEndOverlap_Delimiter</property>
        </cell>
        <cell x="16" y="0" w="4" h="2">
            <text>&#x230A;</text>
            <property name="insert">&#x230A;</property>
            <property name="Name">Bottom begin overlap</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#BottomBeginOverlap_Delimiter</property>
        </cell>
        <cell x="20" y="0" w="4" h="2">
            <text>&#x230B;</text>
            <property name="insert">&#x230B;</property>
            <property name="Name">Bottom end overlap</property>
            <property name="doc">https://talkbank.org/manuals/CHAT.html#BottomEndOverlap_Delimiter</property>
        </cell>
    </grid>


</ipaGrids>