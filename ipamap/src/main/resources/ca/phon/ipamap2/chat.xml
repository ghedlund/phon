<ipaGrids xmlns="http://phon.ling.mun.ca/ns/ipamap">
     <grid rows="4" cols="20" name="Word Prefix">
         <cell x="0" y="0" w="10" h="2">
             <text>0 - omission</text>
             <property name="insert">0$$word$$</property>
             <property name="Name">omission</property>
         </cell>
         <cell x="10" y="0" w="2" h="2">
             <text>&amp;~</text>
             <property name="Name">non word</property>
        </cell>
         <cell x="0" y="2" w="2" h="2">
             <text>&amp;-</text>
             <property name="Name">filler</property>
         </cell>
         <cell x="2" y="2" w="2" h="2">
            <text>&amp;+</text>
            <property name="Name">fragment</property>
         </cell>
     </grid>

    <grid rows="4" cols="20" name="Word Suffix">
        <cell x="0" y="0" w="4" h="2">
            <text></text>
            <property name="Name">fragment</property>
        </cell>
        <cell x="0" y="2" w="4" h="2">
            <text>+&amp;</text>
            <property name="Name"></property>
        </cell>
    </grid>
</ipaGrids>