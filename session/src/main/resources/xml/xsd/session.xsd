<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
       targetNamespace="https://phon.ca/ns/session"
       xmlns="https://phon.ca/ns/session"
       id="PhonBank" version="1.3">

    <xs:annotation>
        <xs:documentation>XML format specification for <a href="https://phon.ca">Phon</a></xs:documentation>
        <xs:documentation>Uses modified elements from the <a href="http://childes.talkbank.org/manuals/chat.html">CHAT Transcription Format</a>
            <a href="https://talkbank.org/software/talkbank.xsd">XML Schema</a> (version 2.20.0)
        </xs:documentation>
    </xs:annotation>

    <xs:element name="session" type="SessionType">
        <xs:key name="uIDKey">
            <xs:selector xpath="transcript/r"/>
            <xs:field xpath="@uuid"/>
        </xs:key>

        <xs:key name="partIDKey">
            <xs:selector xpath="participants/participant"/>
            <xs:field xpath="@id"/>
        </xs:key>

        <xs:keyref refer="partIDKey" name="partIDKeyRef">
            <xs:selector xpath="transcript/r"/>
            <xs:field xpath="@speaker"/>
        </xs:keyref>

        <xs:key name="transcriberKey">
            <xs:selector xpath="transcribers/transcriber"/>
            <xs:field xpath="@id"/>
        </xs:key>

        <xs:keyref refer="transcriberKey" name="transcriberTargetKeyRef">
            <xs:selector xpath="transcript/r/ipaTarget/blindTranscription"/>
            <xs:field xpath="@transcriber"/>
        </xs:keyref>

        <xs:keyref refer="transcriberKey" name="transcriberActualKeyRef">
            <xs:selector xpath="transcript/r/ipaActual/blindTranscription"/>
            <xs:field xpath="@transcriber"/>
        </xs:keyref>

        <xs:key name="userTierKey">
            <xs:selector xpath="userTiers/userTier"/>
            <xs:field xpath="@tierName"/>
        </xs:key>

        <xs:keyref refer="userTierKey" name="groupTierKeyRef">
            <xs:selector xpath="transcript/r/flatTier"/>
            <xs:field xpath="@tierName"/>
        </xs:keyref>

        <xs:keyref refer="userTierKey" name="recTierKeyRef">
            <xs:selector xpath="transcript/r/groupTier"/>
            <xs:field xpath="@tierName"/>
        </xs:keyref>
    </xs:element>

    <xs:element name="date" type="xs:date"/>
    <xs:element name="languages" type="listOfLanguages"/>
    <xs:element name="media" type="xs:string"/>
    <xs:complexType name="SessionType">
        <xs:annotation>
            <xs:documentation>A recording session.</xs:documentation>
            <xs:documentation>
                <a href="https://www.phon.ca/phon-manual/project_manager/sessions.html">Phon manual section on this topic...</a>
            </xs:documentation>
            <xs:documentation>
                <a href="https://www.phon.ca/phon-manual/session_editor/session_editor.html">Phon manual section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <a href="https://www.phon.ca/phon-manual/session_editor/session_information_view.html">Phon manual section on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="languages" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <a href="https://www.phon.ca/phon-manual/session_editor/session_information_view.html">Phon manual section on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="media" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <a href="https://www.phon.ca/phon-manual/session_editor/session_information_view.html">Phon manual section on this topic...</a>
                    </xs:documentation>
                    <xs:documentation>
                        <a href="https://www.phon.ca/phon-manual/session_editor/editor_media.html">Phon manual section on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="participants" minOccurs="0"/>
            <xs:element ref="transcribers" minOccurs="0"/>
            <xs:element ref="userTiers" minOccurs="0"/>
            <xs:element ref="tierOrder" minOccurs="0"/>
            <xs:element ref="transcript"/>
            <xs:element ref="timeline" minOccurs="0"/>
        </xs:sequence>

        <!-- ID = session name in Phon -->
        <xs:attribute name="name" type="nonemptyStringType"/>
        <xs:attribute name="corpus" type="nonemptyStringType"/>
        <xs:attribute name="version" type="xs:string"/>
    </xs:complexType>

    <xs:element name="participants" type="participantsType">
        <xs:annotation>
            <xs:documentation>Participants in session</xs:documentation>
            <xs:documentation>
                <a href="https://www.phon.ca/phon-manual/session_editor/participants.html">Phon manual section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="participantsType" mixed="false">
        <xs:sequence>
            <xs:element ref="participant" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="listOfLanguages">
        <xs:list itemType="xs:language"/>
    </xs:simpleType>

    <xs:element name="participant" type="participantType"/>
    <xs:element name="name" type="xs:string"/>
    <xs:element name="role" type="participantRoleType"/>
    <xs:element name="group" type="xs:string"/>
    <xs:element name="sex" type="sexType"/>
    <xs:element name="SES" type="xs:string"/>
    <xs:element name="education" type="xs:string"/>
    <xs:element name="birthday" type="xs:date"/>
    <xs:element name="birthplace" type="xs:string"/>
    <xs:element name="age" type="ageType"/>
    <xs:element name="first-language" type="xs:language"/>
    <xs:element name="syllabifier" type="xs:string"/>
    <xs:element name="dictionary" type="xs:string"/>
    <xs:element name="other" type="xs:string"/>
    <xs:complexType name="participantType" mixed="false">
        <xs:sequence>
            <xs:element ref="role" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="name" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="group" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="sex" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SES" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="education" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="birthday" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="birthplace" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="age" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="languages" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="first-language" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="syllabifier" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="dictionary" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="other" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <!-- Changed id to xs:ID for constraints -->
        <xs:attribute name="id" type="xs:ID" use="required"/>
    </xs:complexType>

    <xs:simpleType name="participantRoleType">
        <xs:annotation>
            <xs:documentation>List of possible participant roles</xs:documentation>
            <xs:documentation>
                <a href="http://childes.talkbank.org/manuals/chat.html#Role">CHAT manual section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Target_Child"/>
            <xs:enumeration value="Target_Adult"/>
            <xs:enumeration value="Child"/>
            <xs:enumeration value="Mother"/>
            <xs:enumeration value="Father"/>
            <xs:enumeration value="Brother"/>
            <xs:enumeration value="Sister"/>
            <xs:enumeration value="Sibling"/>
            <xs:enumeration value="Grandfather"/>
            <xs:enumeration value="Grandmother"/>
            <xs:enumeration value="Relative"/>
            <xs:enumeration value="Participant"/>
            <xs:enumeration value="Therapist"/>
            <xs:enumeration value="Informant"/>
            <xs:enumeration value="Subject"/>
            <xs:enumeration value="Investigator"/>
            <xs:enumeration value="Partner"/>
            <xs:enumeration value="Boy"/>
            <xs:enumeration value="Girl"/>
            <xs:enumeration value="Adult"/>
            <xs:enumeration value="Teenager"/>
            <xs:enumeration value="Male"/>
            <xs:enumeration value="Female"/>
            <xs:enumeration value="Visitor"/>
            <xs:enumeration value="Friend"/>
            <xs:enumeration value="Playmate"/>
            <xs:enumeration value="Caretaker"/>
            <xs:enumeration value="Environment"/>
            <xs:enumeration value="Group"/>
            <xs:enumeration value="Unidentified"/>
            <xs:enumeration value="Uncertain"/>
            <xs:enumeration value="Other"/>
            <xs:enumeration value="Text"/>
            <xs:enumeration value="Media"/>
            <xs:enumeration value="PlayRole"/>
            <xs:enumeration value="LENA"/>
            <xs:enumeration value="Justice"/>
            <xs:enumeration value="Attorney"/>
            <xs:enumeration value="Doctor"/>
            <xs:enumeration value="Nurse"/>
            <xs:enumeration value="Student"/>
            <xs:enumeration value="Teacher"/>
            <xs:enumeration value="Host"/>
            <xs:enumeration value="Guest"/>
            <xs:enumeration value="Leader"/>
            <xs:enumeration value="Member"/>
            <xs:enumeration value="Narrator"/>
            <xs:enumeration value="Speaker"/>
            <xs:enumeration value="Audience"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ageType">
        <xs:restriction base="xs:duration">
            <!-- Negative durations are not allowed, must start with 'P' -->
            <xs:pattern value="^P.+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="sexType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="male"/>
            <xs:enumeration value="female"/>
            <xs:enumeration value="unspecified"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="listOfIntegers">
        <xs:list itemType="xs:int"/>
    </xs:simpleType>

    <!-- Blind transcribers -->
    <xs:element name="transcribers" type="transcribersType"/>
    <xs:complexType name="transcribersType" mixed="false">
        <xs:annotation>
            <xs:documentation>Blind transcribers</xs:documentation>
            <xs:documentation>
                <a href="https://www.phon.ca/phon-manual/session_editor/blind_transcription.html?hl=blind%2Ctranscription">Phon manual section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="transcriber" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="transcriber" type="transcriberType"/>
    <xs:element name="password" type="passwordType"/>
    <xs:complexType name="passwordType" mixed="true">
        <xs:attribute name="use" type="xs:boolean" default="false"/>
    </xs:complexType>
    <xs:complexType name="transcriberType" mixed="false">
        <xs:annotation>
            <xs:documentation>Blind transcribers. Passwords are optional as they don't really secure
                any data - just prevents others from possibly overwriting data (or
                cheating.)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="name"/>
            <xs:element ref="password"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="required"/>
    </xs:complexType>

    <!-- User-defined tiers -->
    <xs:element name="tierView" type="tierViewType">
        <xs:annotation>
            <xs:documentation>Tier view settings</xs:documentation>
            <xs:documentation>
                <a href="https://www.phon.ca/phon-manual/session_editor/tier_management_view.html">Phon manual section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="tierViewType" mixed="false">
        <xs:attribute name="tierName" type="xs:string"/>
        <xs:attribute name="visible" type="xs:boolean"/>
        <xs:attribute name="locked" type="xs:boolean" default="false" use="optional"/>
        <xs:attribute name="font" type="xs:string" use="optional" default="default"/>
    </xs:complexType>

    <xs:element name="userTiers" type="userTiersType"/>
    <xs:complexType name="userTiersType" mixed="false">
        <xs:annotation>
            <xs:documentation>@UserTiers; a structure enumerating the user defined
                tiers.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="td" maxOccurs="unbounded"/>
            <xs:element ref="tierAlignmentRules" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="td" type="tierDescriptionType"/>
    <xs:complexType name="tierDescriptionType">
        <xs:annotation>
            <xs:documentation>User defined tiers</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="tierParameters" minOccurs="0" maxOccurs="1"/>
            <xs:choice minOccurs="0" maxOccurs="1">
                <xs:element ref="subtypeDelim" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="subtypeExpr"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="tierName" type="xs:ID"/>
        <xs:attribute name="type" type="userTierTypeType" default="default"/>
        <xs:attribute name="excludeFromAlignment" type="xs:boolean" default="false"/>
    </xs:complexType>
    
    <xs:simpleType name="userTierTypeType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="chat">
                <xs:annotation>
                    <xs:documentation>Tier will use same format as orthography tier (CHAT)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ipa">
                <xs:annotation>
                    <xs:documentation>Tier will use same format as ipa tier</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="phone alignment">
                <xs:annotation>
                    <xs:documentation>Custom phone alignment using user-defined ipa tiers</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="default">
                <xs:annotation>
                    <xs:documentation>Tier will use a simplified parser into types separated by spaces.
                        Comments '[% ...]' and inline-media are also parsed. This is the default for
                        user-defined tiers.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="tierParameters" type="tierParametersType"/>
    <xs:complexType name="tierParametersType">
        <xs:annotation>
            <xs:documentation>Collection of tier parameters</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="param" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="param" type="tierParameterType"/>
    <xs:complexType name="tierParameterType" mixed="true">
        <xs:annotation>
            <xs:documentation>key/value pair for tier settings</xs:documentation>
        </xs:annotation>
        <xs:attribute name="name" type="nonemptyStringType" use="required"/>
    </xs:complexType>

    <xs:element name="tierAlignmentRules" type="tierAlignmentRulesType"/>
    <xs:complexType name="tierAlignmentRulesType">
        <xs:sequence>
            <xs:element ref="tierElementFilter" minOccurs="2" maxOccurs="2"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="tierElementFilter" type="tierElementFilterType"/>
    <xs:complexType name="tierElementFilterType">
        <xs:annotation>
            <xs:documentation>Tier filter for any tier</xs:documentation>
        </xs:annotation>
        <xs:attribute name="tier" type="nonemptyStringType" use="required"/>
    </xs:complexType>

    <xs:element name="orthographyElementFilter" type="orthographyElementFilterType"
        substitutionGroup="tierElementFilter"/>
    <xs:complexType name="orthographyElementFilterType">
        <xs:annotation>
            <xs:documentation>Orthography element filter for cross tier alignment</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tierElementFilterType">
                <xs:sequence>
                    <xs:element xmlns="https://phon.ca/ns/session" name="alignWith" type="listOfAlignableOrthographyTypes"/>
                </xs:sequence>
                <xs:attribute name="includeOmitted" type="xs:boolean" default="false">
                    <xs:annotation>
                        <xs:documentation>Include words which have been marked as omitted '0word'</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="includeXXX" type="xs:boolean" default="false">
                    <xs:annotation>
                        <xs:documentation>Include xxx (untranscribed)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="includeYYY" type="xs:boolean" default="false">
                    <xs:annotation>
                        <xs:documentation>Include yyy</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="includeWWW" type="xs:boolean" default="false">
                    <xs:annotation>
                        <xs:documentation>Include ww</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="includeExcluded" type="xs:boolean" default="false">
                    <xs:annotation>
                        <xs:documentation>Include excluded content '[e]'</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:simpleType name="listOfAlignableOrthographyTypes">
        <xs:list itemType="alignableOrthographyTypes"/>
    </xs:simpleType>

    <xs:simpleType name="alignableOrthographyTypes">
        <xs:annotation>
            <xs:documentation>List of alignment utterance types</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="linker"/>
            <xs:enumeration value="word"/>
            <xs:enumeration value="pause"/>
            <xs:enumeration value="group">
                <xs:annotation>
                    <xs:documentation>When selected, only one type is aligned for the entire group, regardless
                    of other selections.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="phoneticGroup">
                <xs:annotation>
                    <xs:documentation>When selected, only one type is aligned for the entire phonetic group, regardless
                        of other selections.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="internal-media"/>
            <xs:enumeration value="tagMarker"/>
            <xs:enumeration value="terminator"/>
            <xs:enumeration value="action"/>
            <xs:enumeration value="happening"/>
            <xs:enumeration value="otherSpokenEvent"/>
            <xs:enumeration value="freecode"/>
            <xs:enumeration value="postcode"/>
            <xs:enumeration value="longFeature"/>
            <xs:enumeration value="nonvocal"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="IPATierElementFilter" type="IPATierElementFilterType" substitutionGroup="tierElementFilter"/>
    <xs:complexType name="IPATierElementFilterType">
        <xs:complexContent>
            <xs:extension base="tierElementFilterType">
                <xs:sequence>
                    <xs:element xmlns="https://phon.ca/ns/session" name="alignWith" type="listOfAlignableIPATierTypes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:simpleType name="listOfAlignableIPATierTypes">
        <xs:list itemType="alignableIPATierTypes"/>
    </xs:simpleType>
    <xs:simpleType name="alignableIPATierTypes">
        <xs:restriction base="xs:string">
            <xs:enumeration value="word"/>
            <xs:enumeration value="pause"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="uerTierElementFilter" type="userTierElementFilterType"/>
    <xs:element name="subtypeDelim" type="nonemptyStringType"/>
    <xs:element name="subtypeExpr" type="nonemptyStringType"/>
    <xs:complexType name="userTierElementFilterType">
        <xs:complexContent>
            <xs:extension base="tierElementFilterType">
                <xs:sequence>
                    <xs:element xmlns="https://phon.ca/ns/session" name="alignWith" type="listofAlignableUserTierTypes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:simpleType name="listofAlignableUserTierTypes">
        <xs:list itemType="alignableUserTierTypes"/>
    </xs:simpleType>
    <xs:simpleType name="alignableUserTierTypes">
        <xs:restriction base="xs:string">
            <xs:enumeration value="type"/>
            <xs:enumeration value="internal-media"/>
            <xs:enumeration value="comment"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="tierOrder" type="tierOrderType"/>
    <xs:complexType name="tierOrderType" mixed="false">
        <xs:annotation>
            <xs:documentation>TierOrder; defining the tier ordering and visibility for the
                session.</xs:documentation>
            <xs:documentation>
                <a href="https://www.phon.ca/phon-manual/session_editor/tier_management_view.html">Phon manual section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="tierView" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="transcript" type="transcriptType"/>
    <xs:complexType name="transcriptType" mixed="false">
        <xs:annotation>
            <xs:documentation>A session transcript consists of interleaved comments, gems, and records.</xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="r"/>
            <xs:element ref="comment"/>
            <xs:element ref="gem"/>
        </xs:choice>
    </xs:complexType>

    <!-- Gems -->
    <xs:element name="gem" type="gemType"/>
    <xs:complexType name="gemType" mixed="true">
        <xs:annotation>
            <xs:documentation>Beginning of a 'gem' (used for processing by the CLAN program GEM)</xs:documentation>
        </xs:annotation>
        <xs:attribute name="type" use="required" type="beginEndLazyType"/>
    </xs:complexType>

    <xs:simpleType name="beginEndLazyType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="begin">
                <xs:annotation>
                    <xs:documentation> Begin a gem (requires matching end of gem). </xs:documentation>
                    <xs:documentation>
                        <a href="https://talkbank.org/manuals/CHAT.html#Bg_Header">CHAT manual section
                            on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="end">
                <xs:annotation>
                    <xs:documentation> End a gem (requires earlier begin of gem). </xs:documentation>
                    <xs:documentation>
                        <a href="https://talkbank.org/manuals/CHAT.html#Eg_Header">CHAT manual section
                            on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="lazy">
                <xs:annotation>
                    <xs:documentation> Label for a begin/end gem. </xs:documentation>
                    <xs:documentation>
                        <a href="https://talkbank.org/manuals/CHAT.html#Bg_Header">CHAT manual
                            section on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- Comments - Comments types are derived from the CHAT transcription format -->
    <xs:element name="comment" type="commentType"/>
    <xs:complexType name="commentType">
        <xs:annotation>
            <xs:documentation>Comment inside transcript</xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="1" maxOccurs="1">
            <xs:element ref="tierData"/>
            <xs:element ref="unparsed"/>
        </xs:choice>
        <xs:attribute type="commentTypeType" name="type" use="required"/>
    </xs:complexType>

    <xs:simpleType name="commentTypeType">
        <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
            <xs:documentation> The allowable types of comment headers. </xs:documentation>
            <xs:documentation>
                <a href="https://talkbank.org/manuals/CHAT.html#Comment_Header">CHAT manual section on
                    this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Activities">
                <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                    <xs:documentation> Activities. </xs:documentation>
                    <xs:documentation>
                        <a href="https://talkbank.org/manuals/CHAT.html#Activities_Header">CHAT manual
                            section on this topic...</a>
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="Bck">
                <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                    <xs:documentation> Bck. </xs:documentation>
                    <xs:documentation>
